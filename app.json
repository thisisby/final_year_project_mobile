{
  "expo": {
    "name": "Fit Guide",
    "slug": "fintess",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "fitguide",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.by.fitguide",
      "buildNumber": "1",
      "version": "1.0.0",
      "icon": "./assets/images/logo.png",
      "splash": {
        "image": "./assets/images/back.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access to take photos.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },

      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/back.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "stateAlwaysHidden|adjustPan",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access to take photos.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/back.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4b745298-66cf-4514-9e72-565a108e0dd2"
      }
    },
    "owner": "baistan",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4b745298-66cf-4514-9e72-565a108e0dd2"
    }
  }
}
